<?xml version="1.0" encoding="utf-8"?>
<odoo> 
    <record id="sale_product_language_import_wizard_view" model="ir.ui.view">
        <field name="name">sale.product.language.import.wizard</field>
        <field name="model">sale.product.language.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Product Wizard">
                <group>
                    <field name="database_attachment"/>
                </group>
                <footer>
                    <button string="Import Languages" type="object" name="import_languages" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
